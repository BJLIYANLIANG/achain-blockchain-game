<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.achain.blockchain.game.mapper.BlockchainDogOrderMapper">

  <resultMap id="BaseResultMap" type="com.achain.blockchain.game.domain.entity.BlockchainDogOrder">
    <id column="id" property="id"/>
    <result column="dog_id" property="dogId"/>
    <result column="order_id" property="orderId"/>
    <result column="seller" property="seller"/>
    <result column="buyer" property="buyer"/>
    <result column="starting_price" property="startingPrice"/>
    <result column="ending_price" property="endingPrice"/>
    <result column="trans_price" property="transPrice"/>
    <result column="status" property="status"/>
    <result column="error_message" property="errorMessage"/>
    <result column="trx_id" property="trxId"/>
    <result column="end_time" property="endTime"/>
    <result column="begin_time" property="beginTime"/>
    <result column="create_time" property="createTime"/>
    <result column="update_time" property="updateTime"/>
  </resultMap>


  <!-- 通用查询结果列-->
  <sql id="Base_Column_List">
    id,dog_id AS dogId,order_id AS orderId,seller,buyer,starting_price AS startingPrice,
    ending_price AS endingPrice,trans_price AS transPrice,status,error_message AS errorMessage,
    trx_id AS trxId,end_time AS endTime,begin_time AS beginTime,
    create_time AS createTime,udpateTime AS updateTime
  </sql>
</mapper>
